<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Dictionary</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
<% if (locals.dictionary) { %>
    <% dictionary.forEach(function (el) { %>
        <h1><%= el.word %></h1>
        <h3>Definition:</h3>
        <p class="a"><%= el.def %></p>
    <% }); %>
<% } %>
<% if (locals.message) { %>
    <h2><%= message %></h2>
<% } %>
</body>
</html>